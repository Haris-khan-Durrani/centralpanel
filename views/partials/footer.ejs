<script>
// Function to get the last part of the URL path
function getLastUrlSegment() {
  const pathArray = window.location.pathname.split('/');
  return pathArray[pathArray.length - 1];
}

// Function to set the dropdown value based on the slug
function setDropdownValue() {
  const slug = getLastUrlSegment();
  const ctasDropdown = document.getElementById('ctas');

  // Set the dropdown value to match the slug
  if (ctasDropdown) {
    ctasDropdown.value = slug;
  }
}

// Call the function on page load
window.onload = function() {
  setDropdownValue();
};

// Form submission logic (as explained previously)
document.getElementById('myForm').onsubmit = function(e) {
  e.preventDefault(); // Prevent the default form submission
  
  const ctas = document.getElementById('ctas').value;
  const cname = document.querySelector('input[name="cname"]').value;

  let url = `https://checklist.crmsoftware.ae/dashboard/${ctas}?cname=${encodeURIComponent(cname)}`;

  window.location.href = url;
};
</script>


